<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myappcompany.rajan.zeta">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Zeta">
        <activity android:name=".DependentDetailsActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".CodeScannerActivity" android:screenOrientation="portrait"/>
        <activity android:name=".SecondaryUserHomeActivity" android:screenOrientation="portrait"/>
        <activity
            android:name=".PrimaryUserHomeActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_primary_user_home" />
        <activity android:name=".SecondaryUserLoginActivity" android:screenOrientation="portrait"/>
        <activity android:name=".PrimaryUserLoginActivity" android:screenOrientation="portrait"/>
        <activity android:name=".EntryActivity" android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>